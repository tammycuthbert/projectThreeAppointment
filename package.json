{
    "name": "project-three-appointment",
    "version": "1.0.0",
    "description": "App where any user can set up appointments online and/or offline. Employees can login to their respective department and create their own schedule.",
    "main": "server.js",
    "scripts": {
        "client-install": "npm install --prefix client",
        "start": "node server.js",
        "server": "nodemon server.js",
        "client": "npm start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "keywords": [
        "mern",
        "mern-app",
        "authentication",
        "authentication-app",
        "appointment",
        "appointment-app",
        "schedule",
        "scheduling",
        "scheduling-app",
        "heroku"
    ],
    "author": "Tammy Cuthbert",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "concurrently": "^5.3.0",
        "express": "^4.17.1",
        "is-empty": "^1.2.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.10.8",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "validator": "^13.1.17"
    },
    "devDependencies": {
        "nodemon": "^2.0.4"
    }
}
